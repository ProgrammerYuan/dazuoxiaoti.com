= stylesheet_link_tag "project_style"
.columns
  - if flash[:notice]
    .notice= flash[:notice]
  .project_left
    .project_info
      = render :partial => @project, :locals => {:main_id => @project.id}
    = render :partial => 'projects/others', :locals => {:projects => @projects[0,4]}
  .project_right
    = render :partial => 'tab_on_project_page', :locals => {:project => @project, :on_question_page => false}
  .clear
